//조건에 맞는 개발자 찾기
SELECT ID, EMAIL, FIRST_NAME, LAST_NAME
FROM DEVELOPERS
WHERE SKILL_CODE & (SELECT CODE FROM SKILLCODES WHERE NAME = 'C#') OR SKILL_CODE & (SELECT CODE FROM SKILLCODES WHERE NAME = 'Python')
ORDER BY ID ASC;
//연도별 대장균 크기의 편차 구하기
SELECT YEAR(E.DIFFERENTIATION_DATE) AS YEAR, ABS(E.SIZE_OF_COLONY - ET.MAX_SIZE) AS YEAR_DEV, E.ID
FROM ECOLI_DATA E LEFT JOIN (SELECT YEAR (E.DIFFERENTIATION_DATE) AS YEAR, MAX(E.SIZE_OF_COLONY) AS MAX_SIZE
                            FROM ECOLI_DATA E
                            GROUP BY YEAR(E.DIFFERENTIATION_DATE)) ET ON YEAR(E.DIFFERENTIATION_DATE) = ET.YEAR
ORDER BY YEAR ASC, YEAR_DEV ASC; 

## 테이블에서 연도별 최대 크기 구하기
## 테이블과 위에서 구한 값 JOIN
## SELECT문으로 크기 편차 구하기
## ORDER BY
//부모의 형질을 모두 가지는 대장균 찾기
SELECT E.ID, E.GENOTYPE, ET.GENOTYPE AS PARENT_GENOTYPE
FROM ECOLI_DATA E, ECOLI_DATA ET
WHERE E.PARENT_ID = ET.ID AND ET.GENOTYPE & E.GENOTYPE = ET.GENOTYPE
ORDER BY ID;
