//오랜 기간 보호한 동물(1)
SELECT AI.NAME, AI.DATETIME
FROM ANIMAL_INS AI LEFT JOIN ANIMAL_OUTS AO ON AI.ANIMAL_ID = AO.ANIMAL_ID
WHERE AO.DATETIME IS NULL
ORDER BY AI.DATETIME LIMIT 3;
//오랜 기간 보호한 동물(2)
SELECT AI.ANIMAL_ID, AI.NAME
FROM ANIMAL_INS AI LEFT JOIN ANIMAL_OUTS AO ON AI.ANIMAL_ID = AO.ANIMAL_ID
ORDER BY AO.DATETIME - AI.DATETIME DESC LIMIT 2;
//대여 기록이 존재하는 자동차 리스트 구하기
SELECT DISTINCT(C.CAR_ID)
FROM CAR_RENTAL_COMPANY_CAR C LEFT JOIN CAR_RENTAL_COMPANY_RENTAL_HISTORY RH ON C.CAR_ID = RH.CAR_ID
WHERE C.CAR_TYPE = '세단' AND DATE_FORMAT(RH.START_DATE, '%m') = '10'
ORDER BY CAR_ID DESC;
